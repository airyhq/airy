{
  "bracketSpacing": false,
  "singleQuote": true,
  "jsxBracketSameLine": true,
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.{ts,tsx}",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.md",
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve",
        "trailingComma": "none"
      }
    }
  ]
}
